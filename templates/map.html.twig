<!DOCTYPE html>
<html>

<head>
	<meta charset="UTF-8">
	<title>Google Map Services</title>
	{% block stylesheets %}
	<link href="{{ asset('assets/css/map.css') }}" rel="stylesheet" />
	{% endblock %}    </head>
	<body>
		<div id="map"></div>
		
		<h3>Locate Address</h3>
		<input id="addr" type="text">
		<button class="btn" id="btnCoord" type="button"><span>Find</span></button>
		<div id="coordInfo"></div>			
		
		<script
		src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDEJApn1weBvRmXgpTTvhkpbPrfUiXPkM8&callback=showMap&libraries=geometry" async defer>
		</script>
		{% block javascripts %}
		<script src="{{ asset('assets/js/map.js') }}"></script>
		{% endblock %}
	</body>
	</html>
